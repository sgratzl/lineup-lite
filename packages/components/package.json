  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/sgratzl/lineup-lite",
  "bugs": {
    "url": "https://github.com/sgratzl/lineup-lite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/lineup-lite.git"
  },
  "style": "dist/components.css",
  "module": "dist/components.esm.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "compile": "tsc -p tsconfig.build.json --noEmit",
    "start": "tsdx watch --tsconfig tsconfig.build.json --noClean --formats cjs",
    "watch": "tsdx watch --tsconfig tsconfig.build.json --noClean --formats cjs",
    "clean": "rimraf dist docs",
    "build": "npm run build:impl && npm run typings",
    "build:impl": "tsdx build --tsconfig tsconfig.build.json",
    "typings": "rollup -c rollup.dts.config.js",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --passWithNoTests --watch",
    "test:coverage": "tsdx test --passWithNoTests --coverage",
    "lint": "npm run eslint && npm run prettier",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier:impl": "prettier \"*.{md,json,js}\" \"{src,sample}/**\" \".storybook/*.js\"",
    "prettier:write": "npm run prettier:impl -- --write",
    "prettier": "npm run prettier:impl -- --check",
    "eslint": "tsdx lint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 8080",
    "docs": "build-storybook --quiet -o ./docs"
  },
  "browserslist": [
    "defaults"
  ],
  "peerDependencies": {
    "@types/react": "^16.9.48",
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@storybook/addon-docs": "^6.0.19",
    "@storybook/addons": "^6.0.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.19",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-testing-library": "^3.7.0",
    "prettier": "^2.1.1",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.6",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-dts": "^1.4.12",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "ts-loader": "^8.0.3",
    "tsdx": "~0.13.3",
    "tslib": "^2.0.1",
    "typedoc": "^0.18.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  }
}
