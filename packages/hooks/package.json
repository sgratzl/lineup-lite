{
  "name": "@lineup-lite/hooks",
  "description": "LineUp-lite is an extension of react-table for rendering data visualization tables inspired by the LineUp visualization technique",
  "version": "1.6.0",
  "license": "MPL-2.0",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/sgratzl/lineup-lite",
  "bugs": {
    "url": "https://github.com/sgratzl/lineup-lite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/lineup-lite.git"
  },
  "dependencies": {
    "@lineup-lite/components": "~1.6.0",
    "date-fns": "^2.28.0"
  },
  "main": "dist/hooks.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "styles": "dist/hooks.css",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/hooks.cjs",
      "style": "./dist/hooks.css",
      "types": "./dist/index.d.ts"
    },
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "sideEffects": "dist/hooks.css",
  "files": [
    "dist",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.css"
  ],
  "scripts": {
    "clear": "shx rm -rf build dist docs node_modules \"*.tgz\" \"*.tsbuildinfo\"",
    "compile": "tsc -b tsconfig.c.json",
    "start": "yarn run watch",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:coverage": "jest --passWithNoTests --coverage",
    "lint": "yarn run eslint && yarn run prettier",
    "fix": "yarn run eslint:fix && yarn run prettier:write",
    "prettier:write": "prettier \"*\" \"*/**\" --write",
    "prettier": "prettier \"*\" \"*/**\" --check",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "yarn run eslint --fix",
    "prepare": "yarn run build",
    "docs": "typedoc src/index.ts"
  },
  "peerDependencies": {
    "@types/react": ">=17",
    "@types/react-table": "^7.7.12",
    "react": ">=17",
    "react-table": "^7.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@sgratzl/rollup-plugin-css-only": "^3.2.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.10",
    "@types/react-table": "^7.7.12",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-table": "^7.8.0",
    "rollup": "^2.75.4",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.3",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.16",
    "typescript": "^4.7.2"
  }
}
