{
  "name": "@lineup-lite/hooks",
  "description": "LineUp-lite is an extension of react-table for rendering data visualization tables inspired by the LineUp visualization technique",
  "version": "1.3.0",
  "license": "MPL-2.0",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/sgratzl/lineup-lite",
  "bugs": {
    "url": "https://github.com/sgratzl/lineup-lite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/lineup-lite.git"
  },
  "dependencies": {
    "@lineup-lite/components": "~1.3.0"
  },
  "main": "dist/hooks.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "styles": "dist/hooks.css",
  "sideEffects": "dist/hooks.css",
  "files": [
    "dist",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.css"
  ],
  "scripts": {
    "clear": "shx rm -rf build dist docs node_modules \"*.tgz\" \"*.tsbuildinfo\"",
    "compile": "tsc -b tsconfig.c.json",
    "start": "yarn run watch",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:coverage": "jest --passWithNoTests --coverage",
    "lint": "yarn run eslint && yarn run prettier",
    "fix": "yarn run eslint:fix && yarn run prettier:write",
    "prettier:write": "prettier \"*\" \"*/**\" --write",
    "prettier": "prettier \"*\" \"*/**\" --check",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "yarn run eslint --fix",
    "prepare": "yarn run build",
    "docs": "typedoc src/index.ts"
  },
  "peerDependencies": {
    "@types/react": ">=17",
    "@types/react-table": "^7.0.28",
    "react": ">=17",
    "react-table": "^7.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-table": "^7.0.28",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-table": "^7.6.3",
    "rollup": "^2.40.0",
    "rollup-plugin-css-only": "github:sgratzl/rollup-plugin-css-only#sgratzl/imports",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.2",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.28",
    "typescript": "~4.1.5"
  }
}
